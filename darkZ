local function TUAQE_fake_script() -- TextButton.Script 
	local script = Instance.new('Script', TextButton)

	
	for i,v in pairs(game.Players.LocalPlayer.Character.Humanoid:GetChildren()) do
		if string.find(v.Name,"Scale") and v.Name ~= "HeadScale" then
			repeat wait(HeadGrowSpeed) until game.Players.LocalPlayer.Character.Head:FindFirstChild("OriginalSize")
			game.Players.LocalPlayer.Character.Head.OriginalSize:Destroy()
			v:Destroy()
			game.Players.LocalPlayer.Character.Head:WaitForChild("OriginalSize")
		end
	end
end
coroutine.wrap(TUAQE_fake_script)()
local function QGMVC_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	
	for i,v in pairs(game.Players.LocalPlayer.Character.Humanoid:GetChildren()) do
		if string.find(v.Name,"Scale") and v.Name ~= "HeadScale" then
			repeat wait(HeadGrowSpeed) until game.Players.LocalPlayer.Character.Head:FindFirstChild("OriginalSize")
			game.Players.LocalPlayer.Character.Head.OriginalSize:Destroy()
			v:Destroy()
			game.Players.LocalPlayer.Character.Head:WaitForChild("OriginalSize")
		end
	end
end
coroutine.wrap(QGMVC_fake_script)()
local function BUWMEKA_fake_script() -- Frame.Script 
	local script = Instance.new('Script', Frame)

	frame = script.Parent.Frame
	frame.Draggable = true
	frame.Active = true
	frame.Selectable = true
end
coroutine.wrap(BUWMEKA_fake_script)()
local function CSJQPE_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	-- Script for closing a frame when a button is clicked
	
	local button = script.Parent  -- Assuming this script is a child of the button
	local frame = button.Parent  -- Assuming the frame is the parent of the button
	
	button.MouseButton1Click:Connect(function()
		frame.Visible = false
	end)
	
end
coroutine.wrap(CSJQPE_fake_script)()
